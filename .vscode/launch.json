    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python:Streamlit",
            "type": "debugpy",
            "request": "launch",
            "program": ".venv/bin/streamlit",
            "args": [
                "run",
                "main.py",
                "--server.port",
                "2000",
            ]
        },
        {
            "name": "Indexing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/indexing.py",
            "args": ["all"],
            "console": "integratedTerminal"
        }

    ]
}